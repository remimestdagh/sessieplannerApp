<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="650.0" prefWidth="940.0" style="-fx-background-color: #406E8E;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.GeavanceerdStatistiekSchermController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../fonts/itlablogoKlein.jpg" />
         </image>
      </ImageView>
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <Region layoutX="210.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
            <Pane>
               <children>
                  <ImageView fx:id="imgViewPowerBI" fitHeight="588.0" fitWidth="920.0" layoutX="0.0" layoutY="0.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/PowerBI.JPG" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Button fx:id="btnKeerTerug" mnemonicParsing="false" onAction="#handleKeerTerugAction" text="Terug">
         <font>
            <Font name="Franklin Gothic Medium Italic" size="12.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" />
         </VBox.margin></Button>
   </children>
</VBox>
